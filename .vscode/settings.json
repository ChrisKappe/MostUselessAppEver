{
    "al.enableCodeAnalysis": true,
    "al.codeAnalyzers": [
        "${CodeCop}",
        "${AppSourceCop}",
        "${UICop}"
    ],
    "al.ruleSetPath": "./.codeAnalysis/thisproject.ruleset.json",
    "CRS.OnSaveAlFileAction": "Rename",
    "editor.formatOnSave": false,
    "CRS.AlSubFolderName": "None",
    //"CRS.ObjectNamePrefix": "CRS ",
    "CRS.RemovePrefixFromFilename": true,
    //"CRS.ObjectNameSuffix": " CRS",
    "CRS.RemoveSuffixFromFilename": true,
    "CRS.ExtensionObjectNamePattern": "<Prefix><BaseNameShort>.<ObjectTypeShort>Ext",
    "CRS.DisableDefaultAlSnippets": true
}